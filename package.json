{
  "name": "node-express-modular-architecture",
  "version": "1.0.0",
  "description": "A modular project architecture for node express apis.",
  "main": "./src/server.js",
  "scripts": {
    "test": "jest --config ./jest.config.js --coverage",
    "coverage": "vitest run --coverage",
    "start": "nodemon -L ./src/server.js",
    "lint": "./node_modules/.bin/eslint .",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "lint:fix": "./node_modules/.bin/eslint . --fix",
    "create:component": "node ./cli init:component"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "author": "Sujeet Agrahari",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@hapi/joi-date": "2.0.1",
    "aws-sdk": "^2.1651.0",
    "axios": "1.6.4",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.12",
    "cors": "^2.8.5",
    "express": "4.20.0",
    "express-async-errors": "^3.1.1",
    "express-winston": "^4.2.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "2.29.4",
    "nodemailer": "6.9.9",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "stoppable": "^1.1.0",
    "uuid": "^8.3.2",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "babel-eslint": "^10.1.0",
    "cli-color": "^2.0.4",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsdoc": "^44.2.7",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "js-beautify": "^1.15.1",
    "jsdoc": "^4.0.3",
    "nodemon": "^3.1.4",
    "prettier": "^2.8.8",
    "sequelize-cli": "^6.6.2",
    "sequelize-mock": "^0.10.2",
    "yargs": "^17.7.2"
  },
  "pnpm": {
    "overrides": {
      "sequelize@<6.28.1": ">=6.28.1",
      "sequelize@<6.29.0": ">=6.29.0",
      "hoek@<4.2.1": ">=4.2.1",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "jsonwebtoken@<9.0.0": ">=9.0.0",
      "jsonwebtoken@<=8.5.1": ">=9.0.0"
    }
  }
}